        .ellipsis {
            overflow: hidden;
            height: 100px;
            line-height: 19px;
            margin: 0px;
            /*border: 5px solid #AAA;*/
        }

            .ellipsis:before {
                content: "";
                float: left;
                width: 5px;
                height: 80px;
            }

            .ellipsis > *:first-child {
                float: right;
                width: 100%;
                margin-left: -5px;
            }

            .ellipsis:after {
                content: "\02026";
                box-sizing: content-box;
                -webkit-box-sizing: content-box;
                -moz-box-sizing: content-box;
                float: right;
                position: relative;
                top: -25px;
                left: 100%;
                width: 3em;
                margin-left: -3em;
                padding-right: 5px;
                text-align: right;
                background-size: 100% 100%; /* 512x1 image,gradient for IE9. Transparent at 0% -> white at 50% -> white at 100%.*/
                /*background-image:url(data:image/png;base64,);*/
            }

